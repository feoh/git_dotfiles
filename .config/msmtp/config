account default
account feoh@feoh.org
host smtp.fastmail.com
port 465
from feoh@feoh.org
user feoh@fastmail.com
passwordeval "pass feoh@feoh.org"
auth on
tls on
tls_trust_file	/etc/ssl/cert.pem
logfile /Users/cpatti/.local/state/msmtp/msmtp.log
tls_starttls off

account cpatti@mit.eduj
host outgoing.mit.edu
port 587
from cpatti@mit.edu
user cpatti
passwordeval "pass cpatti@mit.edu"
auth on
tls on
tls_trust_file	/etc/ssl/cert.pem
logfile /Users/cpatti/.local/state/msmtp/mit_msmtp.log
tls_starttls on
